<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">

	<test name="Yahoo login Test">
		<parameter name="env" value="QA" />
		<parameter name="browser" value="chrome" />
		<parameter name="URL" value="https://login.yahoo.com" />
		<parameter name="emailid" value="test@yahoo.com" />
		<classes>
			<class name="TestNG_NAL.ParametersTest" />
		</classes>
	</test>

	<test name="Test">
		<classes>
			<class name="TestNG_NAL.DependsonMethod" />
			<class name="TestNG_NAL.ExpectedExceptions" />
			<class name="TestNG_NAL.GoogleTest" />
			<class name="TestNG_NAL.InvocationCount" />
			<class name="TestNG_NAL.TestNgBasics" />
		</classes>
	</test>
</suite>